<!--<script src="AppPictureSlides.js"></script>-->
<script>

var slideIndex = 1;


showSlides(slideIndex, 'collegetour');

showSlides(slideIndex, 'converter');

showSlides(slideIndex, 'tictactoe');

showSlides(slideIndex, 'raccoongroupproject');


function plusSlides(n, app) {
 	showSlides(slideIndex += n, app);
}

function currentSlide(n, app) {
 	showSlides(slideIndex = n, app);
}

function showSlides(n, app) {
 	var i;

	if ( app.localeCompare("collegetour") === 0 ) {
  		var slides = document.getElementsByClassName("collegetour");


	} else if ( app.localeCompare("converter") === 0 ) {

		var slides = document.getElementsByClassName("converter");
	} else if ( app.localeCompare("tictactoe") === 0 ) {

		var slides = document.getElementsByClassName("tictactoe");
	} else if ( app.localeCompare("raccoongroupproject") === 0 ) {

		var slides = document.getElementsByClassName("raccoongroupproject");
}



  if (n > slides.length) {slideIndex = 1}    
  if (n < 1) {slideIndex = slides.length}
  for (i = 0; i < slides.length; i++) {
      slides[i].style.display = "none";  

  }
  slides[slideIndex-1].style.display = "block";  

}


</script>




